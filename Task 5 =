#include<iostream>
using namespace std;
int main()
{
    int seat=1;
    for(int i=1; i<=5; i++)
    {
        cout<<"Row #"<<i<<endl<<"\t";
        for(int j=1; j<=10; j++)
        {
            cout<<"Seat #"<<seat<<" ";
            seat=seat+1;
        }
        cout<<endl;
    }
    return 0;
}
